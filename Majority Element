Majority Element
----------------------------------------------
class Solution {
    public int majorityElement(int[] nums) {
        int count = 0;
        int candid = 0;

        for (int num : nums) {
            if (count == 0) {
                candid = num;
            }

            if (num == candid) {
                count++;
            } else {
                count--;
            }
        }

        return candid;
    }
}
